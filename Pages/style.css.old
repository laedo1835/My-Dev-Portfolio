/**----GLOBAL----*/
html,
* {
  box-sizing: border-box;
}
.navleft a:hover {
  color: gray !important;
  font-family: montserrat light;
  text-decoration: underline !important;
}

.navright a:hover {
  color: gray !important;
  font-family: montserrat light;
  text-decoration: underline !important;
}

a:-webkit-any-link {
  color: -webkit-link;
  cursor: pointer;
  text-decoration: none;
  color: #e9e9e9;
}

a:hover {
  color: skyblue;
  text-decoration: underline;
}

.navleft a:-webkit-any-link {
  color: -webkit-link;
  cursor: pointer;
  text-decoration: none;
  color: #000;
}

.navright a:-webkit-any-link {
  color: -webkit-link;
  cursor: pointer;
  text-decoration: none;
  color: #000;
}

body {
  font-family: montserrat light, Helvetica, sans-serif;
  font-size: 16px;
  margin: 0;
  background-color: white;
}

.container {
  margin: 0 auto;
  align-items: center;
  max-width: 100%;
  display: flex;
  flex-wrap: wrap;
  border-bottom: 1px solid #000;
  justify-content: flex-end;
}

.container p {
  font-size: 22px;
  margin: 0px 0px 0px 0px;
}

.container h2 {
  margin: 0px;
}



.main {
  height: 30rem;
  padding-top: 0rem;
  background-image: url("");
  background-size: 50%;
  background-repeat: no-repeat;
  background-position: center;
}

.fa-solid {
  font-weight: 200;
  font-family: "Font Awesome 5 Brands";
  display: inline-block;
  font-style: normal;
  font-variant: normal;
  text-rendering: auto;
  height: 100px;
  width: 100px;
}

.fa-turn-up {
  font-weight: 200;
  font-family: "Font Awesome 5 Brands";
  display: inline-block;
  font-style: normal;
  font-variant: normal;
  text-rendering: auto;
  height: 100px;
  width: 100px;
}


/**----==========---Header----============----*/
header {
  height: 100vh;
  background-color: #e9e9e9;
}
.header1  {
  width: 50%;
  height: 80vh;
  max-width: 800px;
  margin: 1.5rem 0rem 1.5rem 0rem;
  padding-left: 2rem;
  display: flex;
  justify-content: flex-end;
  align-items: flex-end;
}

.site-details {
  width: 255px;
  float: left;
  padding-bottom: 35px;
}

.io {
  max-width: 30vw;
  display: inline-flex;
  flex-wrap: nowrap;
  float: right;
  margin: 12.5rem 0rem 0rem 0rem;
}

.io h1 {
  font-size: 160px;
  font-family: montserrat light;
  text-transform: uppercase;
  margin: 1rem 1rem 0rem;
}

.site-details h1 {
  width:245px;
  font-size: 175px;
  font-family: montserrat light;
  margin: 1rem 0rem 1rem 0rem;
}

.site-details h3 {
  margin: 0px 0px 40px 15px;
}

.site-details p {
  margin: 0px 0px 0px 15px;  
}

.empty {
  height: 20vh;
}

.header2 img {
  display: block;
  width: 30%;
  margin: 1.5rem 1.5rem 1.5rem 0rem;
  float: left;
}





/**-------Navigation--------*/
nav {
  background-color: #e9e9e9;
  color: #000;
  display: inline-flex;
  width: 100%;
  height: 5rem;
  position: fixed;
  z-index: 1;
}

#nav-fixed {
  position: fixed;
  top: 0;
}
.nav1 {
  justify-content: flex-end;
  display: flex;
  width: 50vw;
}

.nav1 ul {
  width: 150px;
}

.nav1 li {
  justify-content: center;
}

.nav2 {
  justify-content: space-between;
  display: flex;
  width: 50vw;
  align-items: center;
}

.nav2 li {
  justify-content: flex-start;
}

.nav2 ul {
  width: 150px;
  padding-left: 0px;
}

.navright li {
  font-size: 20px;
  text-transform: uppercase;
}

.navright{
  width: 150px;
}

li {
  padding: 0rem;
  width: 100%;
  display: flex;
  justify-content: flex-end;
}




/**===================Project Code================**/
.supporting,
.rating,
.gallery,
.location {
  margin: 0 auto;
  background-color: #adadad;
}
.projects {
  margin-top: 150px;
  margin-bottom: 150px;
  height: 100vh;
}
.supporting {
  display: flex;
  justify-content: center;
  padding: 0rem 0rem 0rem;
  align-items: stretch;
  min-height: 800px;
  width: 100%;
}

.supporting img {
  padding: 0.75rem 1.25rem;
  width: 80%;
  padding-right: 5%;
}

.self-portrait {
  display: flex;
  align-items: center;
  justify-content: center;
  width: 50%;
}

.statement {
  display:flex;
  align-items: center;
  max-width: 50%;
}

.statement h1{
  margin: 0rem 10rem 0rem 4rem;
}

.statement span {
  font-family: montserrat light;
  color: #474747;

}
#resume {
  display: flex;
  flex-wrap: wrap;
  justify-content: center;
  align-items: center;
  width: 100vw;
  height: 300px;
}
#resume img {
  margin: 0 auto;
  height: 100px;
  max-width: 85px;
  display: flex;
  align-items: center;
  justify-content: center;
}

.rating {
  background-color: #000;
  height: 250px;
  width: 100vw;
  padding: 0 1rem;
  color: white;
  display: flex;
  justify-content: flex-start;
  align-items: flex-start;
}

#contact {
  display: flex;
  justify-content: center;
  align-items: flex-start;
  height: 150px;
  width: 100vw;
} 


.title h2 {
  color: gray;
  margin-top: 50px;
}

.rating h1 {
  font-size: 2.0rem;
}

.details {
  max-width: 150px;
  display:flex;
  justify-content: flex-start;
}

.details h2 {
  color: #e9e9e9;
  font-family: montserrat light;
}

.details p {
  color: #737373;
  font-size: 20px;
}
.gallery img {
  display: block;
  width: 30%;
  margin: 1.5%;
  float: left;
}

.gallery {
  padding: 8rem;
}

/* 3. Sections Styles
======================================== */
section {
  position: relative;
  margin: 0 auto;
  width: 100vw;
  background-color: #e9e9e9;
  z-index: 1;
}


