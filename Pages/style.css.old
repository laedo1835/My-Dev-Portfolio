.description {
  background-color: rgba(0, 0, 0, 0.5);
  color: white;
  margin: 0;
  width: 100%;
  flex-grow: 1;
  position: absolute;
}

.description h3 {
  padding-left: 20px;
  font-size: 11px;
  font-weight: 200;
  text-transform: uppercase;
  color: #dbdbdb;
  margin-bottom: 2px;
  margin-top:25px;
}

.description h4{
  padding-left: 16px;
  font-size: 22px;
  color: #dbdbdb;
  margin-top: 0px;
}

.clearfix {
  clear: both;
}

.location {
  text-align: center;
  margin: 0 auto;
  height: 1000px;
}

.location img {
  width: 100%;
}

footer {
  background-color: #000;
  padding: 0 1rem;
  display: flex;
  align-items: center;
}

footer nav {
  height: 200px;
  width: 100vw;
  background-color: #000;
  color: #fff;
  display: flex;
  align-items: center;
  justify-content: space-around;
}
.bottom-container {
  height: 60px;
}

footer .container {
  display: flex;
  flex-wrap: nowrap;
  align-items: center;
  justify-content: space-around;
  padding: 0em 0;
  margin: 1rem;
  height: 60px;
  width: 100%;
  color: white;
}

.copyright {
  height: 60px;
  width: 95.9vw;
  display: flex;
  align-items: center;
  justify-content: center;
}

.copyright p {
  font-family: montserrat light;
  color: gray;
  font-size: 14px;
}



/**-----==========LIGHTBOX================------*/
.row > .column {
  padding: 0 8px;
}

.row:after {
  content: "";
  display: table;
  clear: both;
}

.column {
  float: left;
  width: 25%;
}

/* The Modal (background) */
.modal {
  display: none;
  position: fixed;
  z-index: 1;
  padding-top: 100px;
  left: 0;
  top: 0;
  width: 100%;
  height: 100%;
  overflow: auto;
  background-color: #adadad;
}

/* Modal Content */
.modal-content {
  position: relative;
  background-color: #fefefe;
  margin: auto;
  padding: 0;
  width: 90%;
  max-width: 1200px;
}

/* The Close Button */
.close {
  color: white;
  position: absolute;
  top: 10px;
  right: 25px;
  font-size: 35px;
  font-weight: bold;
}

.close:hover,
.close:focus {
  color: #999;
  text-decoration: none;
  cursor: pointer;
}

.mySlides {
  display: none;
}

.cursor {
  cursor: pointer;
}


/* On hover, add a black background color with a little bit see-through */
.prev:hover,
.next:hover {
  background-color: rgba(0, 0, 0, 0.8);
}

/* Number text (1/3 etc) */
.numbertext {
  color: #f2f2f2;
  font-size: 12px;
  padding: 8px 12px;
  position: absolute;
  top: 0;
}

a {
  margin-bottom: -4px;
}

.caption-container {
  text-align: center;
  background-color: black;
  padding: 2px 16px;
  color: white;
}

.demo {
  opacity: 0.6;
}

.active,
.demo:hover {
  opacity: 1;
}

a.hover-shadow {
  transition: 0.3s;
}

.hover-shadow:hover {
  box-shadow: 0 4px 8px 0 rgba(0, 0, 0, 0.2), 0 6px 20px 0 rgba(0, 0, 0, 0.19);
}
